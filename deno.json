{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "ssx"
  },
  "lock": false,
  "imports": {
    "ssx/jsx-runtime": "./jsx-runtime.ts"
  },
  "tasks": {
    "test": "deno test -R",
    "bench": "deno bench benchmark/bench.ts",
    "build-types": "deno run -A _scripts/types.ts && deno fmt html.ts",
    "build-npm": "deno run -A _scripts/build_npm.ts"
  },
  "exclude": [
    "CHANGELOG.md",
    "tests/expected.html"
  ]
}
